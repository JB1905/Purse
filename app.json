{
  "expo": {
    "name": "Purse",
    "description": "React Native application to manage expenses",
    "slug": "Purse",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "sdkVersion": "32.0.0",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
