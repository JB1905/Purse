      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.jbiesiada.purse",
      "permissions": ["CAMERA"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC4qoDGm8Ra6Mskf0uhCsferVNsdJOpOTk"
        }
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
